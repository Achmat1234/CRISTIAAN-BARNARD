            $(document).ready(function () {

                getAccordion("#tabs", 768);
                function getAccordion(element_id, screen) {

                    var concat = '';
                    obj_tabs = $(element_id + " li").toArray();
                    obj_cont = $(".tab-content .tab-pane").toArray();
                    jQuery.each(obj_tabs, function (n, val) {
                        concat += '<div id="' + n + '" class="panel panel-default">';
                        concat += '<div class="panel-heading" role="tab" id="heading' + n + '">';
                        concat += '<h4 class="panel-title"><a role="button" data-toggle="collapse" data-parent="#accordion" href="#collapse' + n + '" aria-expanded="false" aria-controls="collapse' + n + '">' + val.innerText + '</a></h4>';
                        concat += '</div>';
                        concat += '<div id="collapse' + n + '" class="panel-collapse collapse" role="tabpanel" aria-labelledby="heading' + n + '">';
                        concat += '<div class="panel-body">' + obj_cont[n].innerHTML + '</div>';
                        concat += '</div>';
                        concat += '</div>';
                    });
                    $("#accordion").html(concat);
                    $("#accordion").find('.panel-collapse:first').addClass("in");
                    $("#accordion").find('.panel-title a:first').attr("aria-expanded", "true");
                    //$(element_id).remove();
                    //$(".tab-content").remove();

                }


                $('.panel-body button').attr("data-target", function (i, origValue) {
                    return origValue + "-mobile";
                });

                $('.panel-body div.collapse').attr("id", function (i, origValue) {
                    return origValue + "-mobile";
                });

                $('#heading3').parent().hide();
                $('#collapse0').removeClass('in');
                $('#collapse4').addClass('in');

                $('.returns-group [data-toggle="collapse"]').click(function () {
                    $('.returns-group .collapse.in').collapse('hide');
                    var target = $(this).data('target');
                    if ($(this).hasClass('open')) {
                        $(this).removeClass('open');
                    }

                    else {
                        $('button.open').removeClass('open');
                        $(this).addClass('open');
                    }

                });
                $('.akeso-tab .returns-group div.collapse').addClass('in');

                $('#toggleMedicalDisciplines').parent().removeClass('active');
                $('#toggleWardsVisiting').parent().addClass('active');
                $('#MedicalDisciplines').removeClass('in').removeClass('active');
                $('#WardsVisiting').addClass('in').addClass('active');

                
                $('.VisitingHours').click(function (e) {
                  
                   
                    $('.nav-tabs li').removeClass('active')

                    $('#toggleWardsVisiting').parent().addClass('active');
                    $('.tab-content div.tab-pane').removeClass('in').removeClass('active');
                    $('#WardsVisiting').addClass('in').addClass('active');

                    $('#WardsVisiting .desktop-version-of-returns:first-child button:first-child').addClass("open");
