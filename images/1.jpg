            function showMedicalDisciplines(hospitalId, serviceGroupId, serviceId, divId, classOfActvatedServiceGroupSection, activatedDiv, setFocus)
            {
                //Deactivate all divs of activated service group section
                var classWhenActivated = classOfActvatedServiceGroupSection + "-active";
                $('.' + classWhenActivated).addClass(classOfActvatedServiceGroupSection).removeClass(classWhenActivated);

                if (activatedDiv !== null) {
                    $(activatedDiv).addClass(classWhenActivated).removeClass(classOfActvatedServiceGroupSection);

                    //if (setFocus == true) {
                    //    if ($(activatedDiv)[0].id) {
                    //        var toFocusId = $(activatedDiv)[0].id;
                    //        window.location = '#' + toFocusId;
                    //    }

                    //}

                }

                var HospitalServiceDetailsUrl = '/DesktopModules/PhoenixApi/api/Hospitals/GetHospitalServicesWithServiceID?hospitalId=' + hospitalId + '&serviceGroupId=' + serviceGroupId + '&serviceId=' + serviceId;

                var resultsBef = "";

                resultsBef += '<div class="loader-box" style="background: #edf1f5 !important; background-color: #edf1f5 !important; position: relative !important; width: auto !important; height: auto !important;">';
                resultsBef += '<div class="loader" id="loader" style="position: relative !important; left: auto !important;"></div>';
                resultsBef += '</div>';
                //document.getElementById(divId).focus();

                var div = $('#' + divId);

                div.empty();

                div.append(resultsBef);

                $(document).ready(function() {
                    $.ajax({
                        url: HospitalServiceDetailsUrl,
                        contentType: "application/json",
                        //data: JSON.stringify(hospitalServiceDetailsParams),
                        //dataType: 'json',
                        type: 'GET',
                        success: function (data) {
                         //   console.log("item - Detail");
                            console.log(data);
                            var first = true;

                            var results = "";
                            var specialityGroupingName = "";
                            //console.log(data);
                            data.forEach(function(item, index, array) {
                               // console.log(item);

                                //if (index == 0)
                                //    results = '<h5>' + speciality + '</h5>';

                                switch (item.ServiceGrouping) {
                                case 'Medical Disciplines':
                                case 'Specialised Facilities':
                                case 'Ancillary Healthcare Facilities':
                                    if (first === true) {
                                        //console.info(speciality)
                                        //console.log(specialityGroupingName);
                                        groupingName = item.Name;
                                        results += '<h5>' + specialityGroupingName + '</h5>';
                                        first = false;
                                    }

                                    var telLine = (typeof item.Tel !== "undefined" && item.Tel !== "")
                                        ? '<div class="clinic-address-left">Tel:</div><div class="clinic-address-right">' +
                                        item.Tel +
                                        '</div>'
                                        : "";
                                    var faxLine = (typeof item.Fax !== "undefined" && item.Fax !== "")
                                        ? '<div class="clinic-address-left">Fax:</div><div class="clinic-address-right">' +
                                        item.Fax +
                                        '</div>'
                                        : "";
                                    //var emailLine = (typeof item.Email !== "undefined" && item.Email !== "") ? '<div class="clinic-address-left">Email:</div><div class="clinic-address-right"><a href="mailto:' + item.Email + '">' + item.Email + '</a></div>' : "";
                                    var emailLine = (typeof item.Email !== "undefined" && item.Email !== "")
                                        ? '<div class="doctor-email-address"><a href="mailto:' +
                                        item.Email +
                                        '">' +
                                        item.Email +
                                        '</a></div>'
                                        : "";

                                    // DLS: Testing...
                                    // DLS Fix doctor name
                                    var nameLine = (typeof item.PreferredName !== "undefined" && item.PreferredName !== "")
                                        ? '<h6>' + item.Name2 + ' (' + item.PreferredName + ')'
                                        : '<h6>' + item.Name2;
                                    if (item.PersonServices !== specialityGroupingName) {
                                        nameLine = nameLine +
                                            '<div class="doctor-person-services"' +
                                            item.PersonServices +
                                            '</div>'
                                    }
                                    nameLine = nameLine + '</h6>'

                                    // DLS Add web addresses
                                    var websiteLine = (typeof item.Website !== "undefined" && item.Website !== "")
                                        ? '<div class="doctor-website-address"><a href="' +
                                        item.Website +
                                        '" target="_new">' +
                                        item.Website +
                                        '</a></div>'
                                            : "";

                                      //  console.log(websiteLine);
                                    var medPagesLinkLine =
                                        (typeof item.MedPagesLink !== "undefined" && item.MedPagesLink !== "")
                                            ? '<div class="doctor-medpages-address"><a href="' +
                                            item.MedPagesLink +
                                            '" target="_new">Medpages</a></div>'
                                            : "";

                                    var info = (telLine + faxLine + emailLine + websiteLine + medPagesLinkLine === ""
                                        ? ""
                                        : telLine + faxLine + emailLine + websiteLine + medPagesLinkLine);
                                    results += nameLine + info + item.ShortSummary + '<!--/arrow-box-results-->';
                                    // alert(results);
                                        console.info(results);

                                    break;

                                case 'General Facilities And Services':
                                case 'Ward Facilities':
                                    if (first === true) {
                                        //console.info(speciality)
                                        results += '<h5>' + item.Name + '</h5>';
                                        first = false;
                                    }

                                    var telLine = "";
                                    var faxLine = "";
                                    var emailLine = "";

                                    var info = (telLine + faxLine + emailLine === "" ? "" : telLine + faxLine + emailLine);
                                    var summary = item.ShortSummary;
                                    summary = (typeof summary != "undefined" && summary != "") ? summary : "";
                                    results += '<h6>' + item.Name2 + '</h6>' + info + summary + '<!--/arrow-box-results-->';
                                    //console.info(results)

                                    break;
                                }

                                if (index === data.length - 1) {
                                    var div = $('#' + divId);
                                    div.empty();
                                    div.append(results);
                                    //console.log(results);
                                    //console.log(document.getElementById(divId));
                                    //document.getElementById(divId).focus();
                                }

                            })

                        },
                        error: function(jqXHR, textStatus, errorThrown) {
                            //$("#Loader").hide();
                            //window.toastr.error("Could not load doctor address information.");

                            //var div = $('#' + divId);
                            //div.empty();

                            console.log("jqXHR: " + jqXHR);
                            console.log("jqXHR.responseText: " + jqXHR.responseText);
                            console.log("Error: " + errorThrown);
                            console.log("textStatus: " + textStatus);
                        }

                    });
                });
            };


